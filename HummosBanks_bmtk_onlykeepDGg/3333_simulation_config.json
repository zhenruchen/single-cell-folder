{
  "manifest": {
    "$BASE_DIR": ".",
    "$OUTPUT_DIR": "$BASE_DIR/3333_output",
    "$INPUT_DIR": "$BASE_DIR/input"
  },
  "target_simulator": "NEURON",
  "run": {
    "tstop": 15000.0,
    "dt": 0.1,
    "dL": 1000.0,
    "spike_threshold": 0.0,
    "nsteps_block": 5000
  },
  "conditions": {
    "celsius": 34.0,
    "v_init": -65.0
  },
  "node_sets": {
    "virt_poisson_CA3": {
      "pop_name": "poisson_CA3"
    },
    "virt_poisson_DG": {
      "pop_name": "poisson_DG"
    },
    "virt_input_EC": {
      "pop_name": "input_EC"
    }
  },
  "inputs": {
    "spikes_poisson_DG": {
      "input_type": "spikes",
      "module": "csv",
      "input_file": "$INPUT_DIR/exp3_input_poisson_DG.csv",
      "node_set": "virt_poisson_DG"
    },
    "spikes_input_EC": {
      "input_type": "spikes",
      "module": "csv",
      "input_file": "$INPUT_DIR/exp3_input_EC.csv",
      "node_set": "virt_input_EC"
    },
    "spikes_poisson_CA3": {
      "input_type": "spikes",
      "module": "csv",
      "input_file": "$INPUT_DIR/exp3_input_poisson_CA3.csv",
      "node_set": "virt_poisson_CA3"
    }
  },
  "output": {
    "log_file": "log.txt",
    "output_dir": "${OUTPUT_DIR}",
    "spikes_file": "spikes.h5",
    "spikes_file_csv": "spikes.csv",
    "overwrite_output_dir": true
  },
  "reports": {
    "syn_report": {
      "cells": [
        0
      ],
      "variable_name": "W_ampa",
      "module": "netcon_report",
      "sections": "all",
      "syn_type": "pyr2pyr"
    }
  },
  "network": "3333_network_config.json"
}